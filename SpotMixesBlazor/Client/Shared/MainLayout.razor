@inherits LayoutComponentBase
@inject IJSRuntime _jsRuntime

<Navbar></Navbar>

@Body

@code
{
    protected override async Task OnInitializedAsync()
    {
        await _jsRuntime.InvokeVoidAsync("addCssNormalPage");
        await _jsRuntime.InvokeVoidAsync("showHtml", "sidenav");
        // Error
        await _jsRuntime.InvokeVoidAsync("removeHtml", "page-misc");
        // Authentication
        await _jsRuntime.InvokeVoidAsync("removeHtml", "authentication");
    }
}
